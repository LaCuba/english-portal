{
  "name": "english-portal",
  "private": true,
  "scripts": {
    "start:site": "turbo dev --filter=site",
    "start:backend": "turbo start --filter=backend",
    "preview:site": "turbo preview --filter=site",
    "dev:backend": "turbo dev --filter=backend",
    "build:site": "turbo build --filter=site",
    "build:backend": "turbo build --filter=backend",
    "prisma:generate": "turbo prisma:generate --filter=backend",
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "8.57.0",
    "prettier": "3.2.5",
    "turbo": "2.0.5"
  },
  "packageManager": "pnpm@9.4.0"
}
